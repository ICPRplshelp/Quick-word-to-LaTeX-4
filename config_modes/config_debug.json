{
  "preamble_path": "config_modes/preamble.txt",
  "allow_proofs": true,
  "allow_citations": true,
  "allow_no_longtable": false,
  "allow_alignments": true,
  "exclude_preamble": false,
  "citation_mode": "apa2",
  "replacement_mode": false,
  "disallow_figures": false,
  "disable_repair": false,
  "hypertarget_remover": true,
  "fix_vectors": true,
  "dollar_sign_equations": false,
  "center_images": true,
  "fix_prime_symbols": true,
  "allow_environments": true,
  "prevent_pdf_exports": false,
  "fix_derivatives": true,
  "replace_font": true,
  "fix_unicode": true,
  "autodetect_align_symbols": true,
  "pdf_engine": "pdflatex",
  "max_line_length": 110,
  "max_line_align": true,
  "fix_texttt": true,
  "combine_aligns": true,
  "table_of_contents": false,
  "special_proofs": false,
  "forbid_images": false,
  "code_lang": "",
  "citation_brackets": false,
  "bibtex_def": "\\usepackage{biblatex}",
  "citation_keyword": "cite",
  "no_secnum": false,
  "start_of_doc_text": "",
  "environments": ["Theorem", "Lemma", "Corollary", "Proposition", "Conjecture", "Remark", "Note", "Claim", "Definition", "Condition", "Problem", "Example", "Proof[NONE]"]
}