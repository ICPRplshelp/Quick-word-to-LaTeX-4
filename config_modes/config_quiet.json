{
  "description": "Similar to config_standard, but minimizes the length of the preamble.",
  "preamble_path": "config_modes/preamble.txt",
  "allow_proofs": true,
  "allow_citations": true,
  "allow_no_longtable": true,
  "big_text": false,
  "small_margins": false,
  "default_date": "\\today",
  "allow_alignments": true,
  "max_page_length": 35,
  "exclude_preamble": false,
  "citation_mode": "apa2",
  "replacement_mode": false,
  "disallow_figures": false,
  "disable_repair": false,
  "hypertarget_remover": true,
  "fix_vectors": true,
  "dollar_sign_equations": false,
  "center_images": true,
  "fix_prime_symbols": true,
  "allow_environments": true,
  "prevent_pdf_exports": false,
  "fix_derivatives": true,
  "replace_font": false,
  "fix_unicode": true,
  "autodetect_align_symbols": true,
  "pdf_engine": "xelatex",
  "max_line_length": 110,
  "max_line_align": true,
  "fix_texttt": false,
  "combine_aligns": true,
  "table_of_contents": false,
  "special_proofs": false,
  "forbid_images": false,
  "erase_pandoc_preamble": true,
  "verbatim_lang": "",
  "start_of_doc_text": "",
  "citation_brackets": false,
  "no_secnum": false,
  "image_float": "H",
  "environments": ["Theorem", "Lemma", "Corollary", "Proposition", "Conjecture", "Remark", "Note", "Claim", "Definition", "Condition", "Problem", "Example"]
}