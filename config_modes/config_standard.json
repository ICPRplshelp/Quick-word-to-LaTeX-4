{
  "description": "This is the default configuration file. I suggest using this if you do not need to submit the tex source.",
  "preamble_path": "config_modes/preamble.txt",
  "auto_numbering": false,
  "big_text": false,
  "small_margins": true,
  "proof_like_environments": [
    "solution"
  ],
  "default_date": "\\today",
  "document_class": "\\documentclass[12pt]{article}",
  "header_level": 0,
  "default_author": "",
  "headings": false,
  "verbatim_lang": "",
  "verbatim_plugin": "minted",
  "verbatim_options": "linenos, breaklines=true, breakaftergroup=true",
  "allow_proofs": true,
  "allow_citations": true,
  "allow_no_longtable": false,
  "citation_mode": "apa2",
  "replacement_mode": false,
  "disallow_figures": false,
  "dollar_sign_equations": false,
  "center_images": true,
  "allow_environments": true,
  "fix_derivatives": true,
  "replace_font": true,
  "pdf_engine": "xelatex",
  "max_line_length": 110,
  "max_line_align": true,
  "fix_texttt": true,
  "table_of_contents": true,
  "forbid_images": false,
  "citation_keyword": "cite",
  "no_secnum": false,
  "erase_pandoc_preamble": false,
  "image_float": "H",
  "start_of_doc_text": "",
  "environments": [
    "Theorem",
    "Lemma",
    "Corollary",
    "Proposition",
    "Conjecture",
    "Remark",
    "Note",
    "Claim",
    "Definition",
    "Condition",
    "Problem",
    "Example"
  ]
}
