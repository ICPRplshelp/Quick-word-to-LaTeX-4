{
  "preamble_path": "config_modes/preamble.txt",
  "allow_proofs": true,
  "allow_citations": false,
  "allow_no_longtable": false,
  "allow_alignments": true,
  "exclude_preamble": false,
  "citation_mode": "apa2",
  "replacement_mode": false,
  "disallow_figures": false,
  "disable_repair": false,
  "hypertarget_remover": true,
  "fix_vectors": true,
  "dollar_sign_equations": true,
  "center_images": true,
  "fix_prime_symbols": true,
  "allow_environments": true,
  "prevent_pdf_exports": false,
  "fix_derivatives": true,
  "replace_font": true,
  "fix_unicode": true,
  "autodetect_align_symbols": true,
  "pdf_engine": "pdflatex",
  "max_line_length": 110,
  "max_line_align": true,
  "fix_texttt": false,
  "combine_aligns": true,
  "table_of_contents": false,
  "special_proofs": false,
  "forbid_images": false,
  "code_lang": "",
  "start_of_doc_text": "\\renewcommand{\\arraystretch}{2}",
  "environments": {
    "Theorem": {
      "env_name": "Theorem",
      "start": "Theorem.",
      "end": "End theorem.",
      "encapsulation": "textbf",
      "initial_newline": true,
      "priority": 3,
      "has_extra_args": true,
      "extra_args_type": "bracket",
      "env_suffix": "{}"
    },
    "Corollary": {
      "env_name": "Corollary",
      "start": "Corollary.",
      "end": "End corollary.",
      "encapsulation": "textbf",
      "initial_newline": true,
      "priority": 3,
      "has_extra_args": false,
      "extra_args_type": "bracket"
    },
    "Lemma": {
      "env_name": "Lemma",
      "start": "Lemma.",
      "end": "End lemma.",
      "encapsulation": "textbf",
      "initial_newline": true,
      "priority": 3,
      "has_extra_args": false,
      "extra_args_type": "bracket"
    },
    "Remark": {
      "env_name": "Remark",
      "start": "Remark.",
      "end": "End remark.",
      "encapsulation": "textbf",
      "initial_newline": false,
      "priority": 3,
      "has_extra_args": false,
      "extra_args_type": "bracket"
    },
    "Definition": {
      "env_name": "Definition",
      "start": "Definition.",
      "end": "End definition.",
      "encapsulation": "textbf",
      "initial_newline": false,
      "priority": 1,
      "has_extra_args": true,
      "extra_args_type": "bracket",
      "env_suffix": "{}"
    },
    "Example": {
      "env_name": "Example",
      "start": "Example.",
      "end": "End example.",
      "encapsulation": "textbf",
      "initial_newline": false,
      "priority": 2,
      "has_extra_args": true,
      "extra_args_type": "bracket",
      "env_suffix": "{}"
    },
    "mytheorem": {
      "env_name": "mytheorem",
      "start": "MT.",
      "end": "End MT.",
      "encapsulation": "textbf",
      "initial_newline": false,
      "priority": 1,
      "has_extra_args": false,
      "extra_args_type": "brace"
    },
    "hintbox": {
      "env_name": "hintbox",
      "start": "Hint.",
      "end": "End hint.",
      "encapsulation": "emph",
      "initial_newline": false,
      "priority": 1,
      "has_extra_args": true,
      "extra_args_type": "brace"
    },
        "infobox": {
          "env_name": "infobox",
      "start": "Info.",
      "end": "End info.",
      "encapsulation": "emph",
      "initial_newline": false,
      "priority": 1,
      "has_extra_args": true,
      "extra_args_type": "brace"
    },
        "warningbox": {
          "env_name": "warningbox",
      "start": "Warning.",
      "end": "End warning.",
      "encapsulation": "emph",
      "initial_newline": false,
      "priority": 1,
      "has_extra_args": true,
      "extra_args_type": "brace"
    }
  }
}